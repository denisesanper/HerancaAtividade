package veiculo.modelo;

public class Veiculo {
	private String roda;
	private String cor;
	private double vmax;
	
	public String getRoda() {
		return roda;
	}
	public void setRoda(String roda) {
		this.roda = roda;
	}
	public String getCor() {
		return cor;
	}
	public void setCor(String cor) {
		this.cor = cor;
	}
	public double getVmax() {
		return vmax;
	}
	public void setVmax(double vmax) {
		this.vmax = vmax;
	}
	
	
}
